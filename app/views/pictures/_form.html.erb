<%= simple_form_for(@picture) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :artist %>
    <%= f.input :url %>
    <%= f.input :artist_id %>
    <%= f.input :category_id %>
    <%= f.association :category %>
    <%= f.association :artist %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
